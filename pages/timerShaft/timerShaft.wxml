<!--pages/timerShaft/timerShaft.wxml-->
<image src='../../assets/images/time.png'></image>
<view wx:for="{{completedTodos}}" wx:key="index" wx:for-item="todo" style='width:100%;height:100%'>
<view class='al-item'>
   <view class='col-line'></view> 
  <view class='shaft-item'>
     <view class='col-line'></view> 
    <view class='shaft-item-time bg-color'>{{todo.completedAt}}</view>
    <view class='dot'>‚óè</view>
    <view class='shaft-item-content bg-color'>
      <view class='shaft-item-title bg-color'>{{todo.title}}</view>
      <view class='shaft-item-desc bg-color'>{{todo.desc}}</view>
    </view>
  </view>
</view>
</view>